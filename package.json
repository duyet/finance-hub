{
  "name": "finance-hub",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "react-router dev",
    "build": "react-router build",
    "start": "wrangler pages dev ./build/client",
    "typecheck": "react-router typegen && tsc",
    "deploy": "bun run build && wrangler pages deploy ./build/client",
    "config": "echo 'Configure secrets via: wrangler pages secret put <SECRET_NAME>' && wrangler pages secret --help",
    "lint": "eslint .",
    "test": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "bun run test && bun run test:e2e",
    "ci": "bun run typecheck && bun run lint && bun run test && bun run build",
    "format": "eslint --fix ."
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-dropdown-menu": "^2.1.4",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-select": "^2.1.4",
    "@radix-ui/react-separator": "^1.1.1",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-toast": "^1.2.4",
    "@react-pdf/renderer": "^4.1.5",
    "@react-router/cloudflare": "^7.11.0",
    "@react-router/fs-routes": "^7.11.0",
    "@react-router/node": "^7.11.0",
    "@types/papaparse": "^5.5.2",
    "arctic": "^3.7.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "i18next": "^24.2.2",
    "i18next-browser-languagedetector": "^8.0.2",
    "isbot": "^5.1.17",
    "lucide-react": "^0.468.0",
    "oslo": "^1.2.1",
    "papaparse": "^5.5.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.69.0",
    "react-i18next": "^15.1.3",
    "react-router": "^7.11.0",
    "recharts": "^2.15.0",
    "tailwind-merge": "^2.6.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@react-router/dev": "^7.11.0",
    "@tailwindcss/vite": "^4.0.0",
    "@types/react": "^19.0.6",
    "@types/react-dom": "^19.0.2",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.57.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.19.1",
    "vite": "^6.0.7",
    "vitest": "^2.1.8",
    "wrangler": "^4.9.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
